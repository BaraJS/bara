{
  "name": "bara-cli",
  "description": "Bara CLI Application - Created For Creating",
  "version": "2.2.0",
  "author": "nampdn @nampdn",
  "homepage": "https://barajs.dev",
  "repository": "barajs/bara",
  "bugs": "https://github.com/barajs/bara/issues",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "bara": "./bin/run"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "bara",
    "cli",
    "framework",
    "application",
    "generator"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "bara",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "lint": "tslint -p .",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS && exit 0;",
    "compile": "echo NO COMPILE && exit 0;",
    "version": "oclif-dev readme && git add README.md",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "cli-ux": "5.2.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.0",
    "@oclif/tslint": "3.1.1",
    "@types/node": "10.14.4",
    "globby": "8.0.2",
    "gts": "0.9.0",
    "prettier": "1.16.4",
    "ts-node": "7.0.1",
    "tslint": "5.13.0",
    "typescript": "3.3.3333"
  },
  "gitHead": "f4c8920813b598869d4e664195ca7838532dd17f"
}
