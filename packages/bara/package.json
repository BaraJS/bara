{
  "name": "bara",
  "version": "3.0.0-alpha.1",
  "author": "nampdn @nampdn",
  "description": "Created For Creating - Create any JavaScript application.",
  "homepage": "https://barajs.dev",
  "repository": "barajs/bara",
  "main": "dist/bara.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "license": "MIT",
  "keywords": [
    "bara",
    "reactive programming",
    "trigger",
    "universal",
    "backend",
    "frontend"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "clean": "rimraf build dist",
    "compile:tsc": "tsc -p ./tsconfig.build.json",
    "compile:webpack": "webpack --mode=production",
    "compile": "yarn clean && yarn compile:tsc && yarn compile:webpack",
    "tsc": "tsc -p .",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "prepare": "yarn compile",
    "pretest": "yarn format",
    "lint": "exit 0"
  },
  "size-limit": [
    {
      "webpack": true,
      "path": "dist/*.js",
      "limit": "1 s"
    }
  ],
  "dependencies": {
    "@bara/core": "^3.0.0-alpha.1"
  },
  "devDependencies": {
    "prettier": "1.18.2",
    "size-limit": "1.3.8",
    "ts-loader": "5.4.5",
    "typescript": "3.6.3",
    "webpack": "4.40.2",
    "webpack-cli": "3.3.8"
  },
  "gitHead": "f4c8920813b598869d4e664195ca7838532dd17f"
}
